{
  "version": 8,
  "name": "Test",
  "metadata": {
    "caching": ["case", ["to-boolean", ["global-state", "offline"]], "do-nothing", "network-only"]
  },
  "sources": {
    "geojson_src": {
      "data": {
        "features": [],
        "type": "FeatureCollection"
      },
      "type": "geojson"
    },
    "raster_src": {
      "metadata": {
        "caching": [
          "case",
          ["all", ["to-boolean", ["global-state", "terrain"]], ["to-boolean", ["global-state", "network"]]],
          "network-only",
          "do-nothing"
        ]
      },
      "type": "raster"
    },
    "vector_src": {
      "metadata": {
        "caching": [
          "case",
          ["to-boolean", ["global-state", "offline"]],
          "cache-only",
          ["all", ["to-boolean", ["global-state", "network"]], ["to-boolean", ["global-state", "noCache"]]],
          "network-only",
          "cache-first-then-cache"
        ]
      },
      "type": "vector",
      "tiles": []
    }
  },
  "layers": [
    {
      "id": "background",
      "paint": {
        "background-color": {
          "stops": [
            [6, "rgba(252, 247, 229, 1)"],
            [10, "rgba(252, 247, 229, 1)"],
            [14, "rgba(246, 241, 229, 1)"],
            [15, "rgba(246, 241, 229, 1)"]
          ]
        }
      },
      "type": "background"
    },
    {
      "id": "park",
      "paint": {
        "fill-color": "hsla(90, 41%, 85%, 1)",
        "fill-opacity": 0.7,
        "fill-outline-color": "hsla(123, 55%, 59%, 1)"
      },
      "source": "vector_src",
      "source-layer": "park",
      "type": "fill"
    },
    {
      "id": "name",
      "layout": {
        "text-field": "{name}"
      },
      "source": "vector_src",
      "source-layer": "waterway",
      "type": "symbol"
    },
    {
      "id": "image",
      "source": "raster_src",
      "source-layer": "image",
      "type": "raster"
    }
  ]
}
